pkglib_LTLIBRARIES = libdmclfc.la

libdmclfc_la_SOURCES = DataPointLFC.cpp DataPointLFC.h
libdmclfc_la_CXXFLAGS = -I$(top_srcdir)/include \
	$(GLOBUS_GSSAPI_GSI_CFLAGS) $(LIBXML2_CFLAGS) $(GLIBMM_CFLAGS) \
	$(LFC_CFLAGS)
libdmclfc_la_LIBADD = \
	$(top_builddir)/src/hed/libs/globusutils/libarcglobusutils.la \
	$(top_builddir)/src/hed/libs/data/libarcdata2.la \
	$(top_builddir)/src/hed/libs/crypto/libarccrypto.la \
	$(top_builddir)/src/hed/libs/message/libarcmessage.la \
	$(top_builddir)/src/hed/libs/loader/libarcloader.la \
	$(top_builddir)/src/hed/libs/common/libarccommon.la \
	$(GLOBUS_GSSAPI_GSI_LIBS) $(GLOBUS_OPENSSL_MODULE_LIBS) \
	$(GLOBUS_COMMON_LIBS) \
	$(LIBXML2_LIBS) $(GLIBMM_LIBS) $(LFC_LIBS)
libdmclfc_la_LDFLAGS = -no-undefined -avoid-version -module

<?xml version="1.0"?>
<ArcConfig xmlns="http://www.nordugrid.org/schemas/ArcConfig/2007">
    <ModuleManager>
        <Path>../../security/.libs/</Path>
    </ModuleManager>
    <Plugins Name="security">
          <Plugin Name="__arc_attrfactory_modules__">attrfactory</Plugin>
          <Plugin Name="__arc_fnfactory_modules__">fnfactory</Plugin>
          <Plugin Name="__arc_algfactory_modules__">algfactory</Plugin>
          <Plugin Name="__arc_xxxfactory_modules__">xxxfactory</Plugin>
    </Plugins>

    <PDPConfig>
          <PolicyStore name="test" location="Policy_Example.xml"/>
          <AttributeFactory name="attr.factory" />
          <CombingAlgorithmFactory name="alg.factory" />
          <FunctionFactory name="fn.factory" />
    </PDPConfig>

</ArcConfig>

#!@posix_shell@
# set -x
# 
#  Cancel job running in SGE.
# 

echo "----- starting cancel_sge_job -----" 1>&2

joboption_lrms=sge

basedir=`dirname $0`
basedir=`cd $basedir > /dev/null && pwd` || exit $?

pkglibdir="$basedir/@pkglibdir_rel_to_pkglibexecdir@"
pkglibdir=`cd $pkglibdir > /dev/null && pwd` || exit $?

. ${pkglibdir}/cancel_common.sh $1 || exit $?

echo executing $SGE_BIN_PATH/qdel with job id $joboption_jobid 1>&2
$SGE_BIN_PATH/qdel "${joboption_jobid}"

echo "----- exiting cancel_sge_job -----" 1>&2
echo "" 1>&2
exit 0








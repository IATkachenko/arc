#include <stdio.h>
#include <unistd.h>
#include <sys/types.h>

/**
 * A perl script cannot gain root proviledges via the UNIX sticky bits,
 * while this C code can.
 */

int main(int argc, char **argv)
{
	setuid(0);
	setgid(0);

	execv("@prefix@/share/arc/perl/Janitor/interfaces/commandline/janitor", argv);

	return 4;
}

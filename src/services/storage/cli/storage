#!/bin/bash
export PYTHONPATH=./arc1/src/services:./arc1/python:./arc1/python/.libs
if [ "$#" == "0" ]
then
    echo "USAGE:"
    echo "  storage hash|catalog|manager|element <command> <arguments>"
    exit 1
fi
if [ "$1" == "hash" ]
then
    shift
    python ./arc1/src/services/storage/cli/hash_client.py $*
elif [ "$1" == "catalog" ]
then
    shift
    python ./arc1/src/services/storage/cli/catalog_client.py $*
elif [ "$1" == "manager" ]
then
    shift
    python ./arc1/src/services/storage/cli/manager_client.py $*
elif [ "$1" == "element" ]
then
    shift
    python ./arc1/src/services/storage/cli/element_client.py $*
fi

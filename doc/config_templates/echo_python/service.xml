    <Service name="pythonservice" id="echo_python">
        <ClassName>echo_python.EchoService.EchoService</ClassName>
        <echo:prefix>[ </echo:prefix>
        <echo:suffix> ]</echo:suffix>
        <register:InfoRegister>
            <!-- These attributes are necessery for the self-registration -->
            <register:Period>PT75S</register:Period>
            <register:Endpoint>NOPE<!-- Your service URL should be here  --></register:Endpoint>
            <register:Expiration>PT5H</register:Expiration>
            <!-- If there is no specific Registrar defined then every InfoRegistrar
             will be use used during the registration. The period of the repeating registration will
             be the value given here in every ISIS cloud. -->
             <!-- Any previously defined attribute can be overwritten per Registrar -->
            <register:Registrar id="NorduGridInfosys2">
                <register:Expiration>PT6H</register:Expiration>
            </register:Registrar>
        </register:InfoRegister>
    </Service>


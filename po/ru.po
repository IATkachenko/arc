#: src/libs/data-staging/Scheduler.cpp:170
#, c-format
msgid "DTR %s: Using next %s replica"
msgstr "DTR %s: Используется следующая копия (%s)"

#: src/libs/data-staging/Scheduler.cpp:177
#, c-format
msgid "DTR %s: No more %s replicas"
msgstr "DTR %s: Больше копий нет (%s)"

#: src/libs/data-staging/Scheduler.cpp:179
#, c-format
msgid "DTR %s: Will clean up pre-registered destination"
msgstr "DTR %s: Предварительное назначение будет сброшено"

#: src/libs/data-staging/Scheduler.cpp:183
#, c-format
msgid "DTR %s: Will release cache locks"
msgstr "DTR %s: Будет отменены блокировки в кэше"

#: src/libs/data-staging/Scheduler.cpp:186
#, c-format
msgid "DTR %s: Moving to end of data staging"
msgstr "DTR %s: Заканчивается размещение данных"

#: src/libs/data-staging/Scheduler.cpp:195
#, c-format
msgid "DTR %s: Source is mapped to %s"
msgstr "DTR %s: Источник поставлен в соответствие %s"

#: src/libs/data-staging/Scheduler.cpp:199
#, c-format
msgid "DTR %s: Cannot link to source which can be modified, will copy instead"
msgstr ""
"DTR %s: Невозможно создать ссылку на источник, который может измениться; "
"будет сделана копия"

#: src/libs/data-staging/Scheduler.cpp:209
#, c-format
msgid "DTR %s: Cannot link to a remote destination. Will not use mapped URL"
msgstr ""
"DTR %s: Невозможно создать ссылку на удалённое назначение. Приписанный URL "
"не будет использован"

#: src/libs/data-staging/Scheduler.cpp:212
#, c-format
msgid "DTR %s: Linking mapped file"
msgstr "DTR %s: Создаётся символическая ссылка на соответствующий файл"

#: src/libs/data-staging/Scheduler.cpp:219
#, c-format
msgid "DTR %s: Failed to create link: %s. Will not use mapped URL"
msgstr "DTR %s: Сбой создания ссылки %s. Приписанный URL не будет использован"

#: src/libs/data-staging/Scheduler.cpp:232
#, c-format
msgid "DTR %s: Linking mapped file - can't link on Windows"
msgstr ""
"DTR %s: Создаётся символическая ссылка на соответствующий файл - невыполнимо "
"на Windows"

#: src/libs/data-staging/Scheduler.cpp:248
#, c-format
msgid ""
"Scheduler received new DTR %s with source: %s, destination: %s, assigned to "
"transfer share %s with priority %d"
msgstr ""
"Планировщик получил новый запрос DTR %s с источником: %s, назначением: %s, "
"приписан к доле %s с приоритетом %d"

#: src/libs/data-staging/Scheduler.cpp:255
#, c-format
msgid ""
"DTR %s: File is not cacheable, was requested not to be cached or no cache "
"available, skipping cache check"
msgstr ""
"DTR %s: Файл либо не может быть кэширован, кэширование не было запрошено, "
"либо кэша нет; пропускается проверка кэша"

#: src/libs/data-staging/Scheduler.cpp:261
#, c-format
msgid "DTR %s: File is cacheable, will check cache"
msgstr "DTR %s: Файл может быть кэширован, кэш будет проверен"

#: src/libs/data-staging/Scheduler.cpp:264
#: src/libs/data-staging/Scheduler.cpp:291
#, c-format
msgid "DTR %s: File is currently being cached, will wait %is"
msgstr "DTR %s: Файл ещё кэшируется, ожидание %i сек"

#: src/libs/data-staging/Scheduler.cpp:284
#, c-format
msgid "DTR %s: Timed out while waiting for cache lock"
msgstr "DTR %s: Истекло время ожидания блокировки кэша"

#: src/libs/data-staging/Scheduler.cpp:296
#, c-format
msgid "DTR %s: Checking cache again"
msgstr "DTR %s: Кэш проверяется заново"

#: src/libs/data-staging/Scheduler.cpp:312
#, c-format
msgid "DTR %s: Destination file is in cache"
msgstr "DTR %s: Файл назначения в кэше"

#: src/libs/data-staging/Scheduler.cpp:316
#, c-format
msgid ""
"DTR %s: Source and/or destination is index service, will resolve replicas"
msgstr ""
"DTR %s: Источник и/или назначение являются указателями, будет произведён "
"поиск копий"

#: src/libs/data-staging/Scheduler.cpp:319
#, c-format
msgid ""
"DTR %s: Neither source nor destination are index services, will skip "
"resolving replicas"
msgstr ""
"DTR %s: Ни источник, ни назначение не являются указателями, поиск копий не "
"будет произведён"

#: src/libs/data-staging/Scheduler.cpp:329
#, c-format
msgid "DTR %s: Problem with index service, will release cache lock"
msgstr "DTR %s: Проблема с указателем, кэш будет разблокирован"

#: src/libs/data-staging/Scheduler.cpp:333
#, c-format
msgid "DTR %s: Problem with index service, will proceed to end of data staging"
msgstr ""
"DTR %s: Проблема с указателем, переходим к завершению размещения данных"

#: src/libs/data-staging/Scheduler.cpp:343
#, c-format
msgid "DTR %s: Checking source file is present"
msgstr "DTR %s: Проверка наличия файла-источника"

#: src/libs/data-staging/Scheduler.cpp:351
#, c-format
msgid "DTR %s: Error with source file, moving to next replica"
msgstr "DTR %s: Ошибка в файле источника, пробуем другую копию"

#: src/libs/data-staging/Scheduler.cpp:368
#, c-format
msgid "DTR %s: Replica %s has long latency, trying next replica"
msgstr "DTR %s: У копии %s долгая задержка, пробуем следующую копию"

#: src/libs/data-staging/Scheduler.cpp:370
#, c-format
msgid "DTR %s: No more replicas, will use %s"
msgstr "DTR %s: Больше копий нет, будет использован файл %s"

#: src/libs/data-staging/Scheduler.cpp:373
#, c-format
msgid "DTR %s: Checking replica %s"
msgstr "DTR %s: Проверяется копия %s"

#: src/libs/data-staging/Scheduler.cpp:383
#, c-format
msgid "DTR %s: Overwrite requested - will pre-clean destination"
msgstr "DTR %s: Запрошена перезапись - назначение будет предварительно очищено"

#: src/libs/data-staging/Scheduler.cpp:386
#, c-format
msgid "DTR %s: No overwrite requested or allowed, skipping pre-cleaning"
msgstr ""
"DTR %s: Перезапись не запрошена или не разрешена, предварительная очистка "
"пропускается"

#: src/libs/data-staging/Scheduler.cpp:394
#, c-format
msgid "DTR %s: Pre-clean failed, will still try to copy"
msgstr "DTR %s: Сбой предварительной очистки, всё же попытаемся скопировать "

#: src/libs/data-staging/Scheduler.cpp:401
#, c-format
msgid "DTR %s: Source or destination requires staging"
msgstr ""
"DTR %s: Источник или назначение требуют размещения с ленточного накопителя"

#: src/libs/data-staging/Scheduler.cpp:405
#, c-format
msgid "DTR %s: No need to stage source or destination, skipping staging"
msgstr ""
"DTR %s: Не требуется размещение с ленточного накопителя ни источника, ни "
"назначения; размещение пропускается "

#: src/libs/data-staging/Scheduler.cpp:435
#, c-format
msgid "DTR %s: Staging request timed out, will release request"
msgstr ""
"DTR %s: Истекло время ожидания запроса на размещение, запрос будет отозван"

#: src/libs/data-staging/Scheduler.cpp:439
#, c-format
msgid "DTR %s: Querying status of staging request"
msgstr "DTR %s: Опрос состояния запроса на размещение"

#: src/libs/data-staging/Scheduler.cpp:448
#, c-format
msgid "DTR %s: Releasing requests"
msgstr "DTR %s: Сброс запросов"

#: src/libs/data-staging/Scheduler.cpp:465
#, c-format
msgid "DTR %s: DTR is ready for transfer, moving to delivery queue"
msgstr "DTR %s: DTR готов к пересылке, переводится в очередь на доставку"

#: src/libs/data-staging/Scheduler.cpp:480
#, c-format
msgid "DTR %s: Transfer failed: %s"
msgstr "DTR %s: Сбой передачи: %s"

#: src/libs/data-staging/Scheduler.cpp:490
#, c-format
msgid "DTR %s: Releasing request(s) made during staging"
msgstr "DTR %s: Отзыв запросов, сделанных при размещении"

#: src/libs/data-staging/Scheduler.cpp:493
#, c-format
msgid ""
"DTR %s: Neither source nor destination were staged, skipping releasing "
"requests"
msgstr ""
"DTR %s: Ни источник, ни назначение не были размещены с ленточного "
"накопителя, пропускается отмена запросов"

#: src/libs/data-staging/Scheduler.cpp:505
#, c-format
msgid "DTR %s: Trying next replica"
msgstr "DTR %s: Пробуем следующую копию"

#: src/libs/data-staging/Scheduler.cpp:509
#, c-format
msgid "DTR %s: Will %s in destination index service"
msgstr "DTR %s: Будет выполнена операция %s в каталоге назначения"

#: src/libs/data-staging/Scheduler.cpp:513
#, c-format
msgid "DTR %s: Destination is not index service, skipping replica registration"
msgstr ""
"DTR %s: Назначение не является указателем, пропускается регистрация копии"

#: src/libs/data-staging/Scheduler.cpp:526
#, c-format
msgid "DTR %s: Error registering replica, moving to end of data staging"
msgstr "DTR %s: Ошибка регистрации копии, переход к завершению размещения"

#: src/libs/data-staging/Scheduler.cpp:534
#, c-format
msgid "DTR %s: Will process cache"
msgstr "DTR %s: Будет обработан кэш"

#: src/libs/data-staging/Scheduler.cpp:538
#, c-format
msgid "DTR %s: File is not cacheable, skipping cache processing"
msgstr "DTR %s: Файл не может быть кэширован, пропускается обработка кэша"

#: src/libs/data-staging/Scheduler.cpp:552
#, c-format
msgid "DTR %s: Cancellation complete"
msgstr "DTR %s: Отмена завершена"

#: src/libs/data-staging/Scheduler.cpp:566
#, c-format
msgid "DTR %s: Will wait 10s"
msgstr "DTR %s: Ожидание 10 секунд"

#: src/libs/data-staging/Scheduler.cpp:572
#, c-format
msgid "DTR %s: Error in cache processing, will retry without caching"
msgstr "DTR %s: Ошибка при обработке кэша, попытаемся без кэширования"

#: src/libs/data-staging/Scheduler.cpp:581
#, c-format
msgid "DTR %s: Will retry without caching"
msgstr "DTR %s: Будет произведена повторная попытка без кэширования"

#: src/libs/data-staging/Scheduler.cpp:598
#, c-format
msgid "DTR %s: %i retries left, will wait until %s before next attempt"
msgstr "DTR %s: Осталось %i попыток, повторная попытка через %s"

#: src/libs/data-staging/Scheduler.cpp:614
#, c-format
msgid "DTR %s: Out of retries"
msgstr "DTR %s: Достигнут предел количества попыток"

#: src/libs/data-staging/Scheduler.cpp:616
#, c-format
msgid "DTR %s: Permanent failure"
msgstr "DTR %s: Устойчивый сбой"

#: src/libs/data-staging/Scheduler.cpp:622
#, c-format
msgid "DTR %s: Finished successfully"
msgstr "DTR %s: Успешное завершение"

#: src/libs/data-staging/Scheduler.cpp:632
#, c-format
msgid "DTR %s: Returning to generator"
msgstr "DTR %s: Возврат в генератор"

#: src/libs/data-staging/Scheduler.cpp:799
#, c-format
msgid "DTR %s: Using non-RFC proxy so only local delivery can be used"
msgstr ""
"DTR %s: Используется несовместимая с RFC доверенность, поэтому возможна "
"только локальная доставка"

#: src/libs/data-staging/Scheduler.cpp:806
#, c-format
msgid "DTR %s: Using non-RFC proxy so forcing local delivery"
msgstr ""
"DTR %s: Используется несовместимая с RFC доверенность, вынужденно "
"используется локальная доставка"

#: src/libs/data-staging/Scheduler.cpp:816
#, c-format
msgid "DTR %s: File is smaller than %llu bytes, will use local delivery"
msgstr "DTR %s: Файл меньше %llu байт, будет использована локальная доставка"

#: src/libs/data-staging/Scheduler.cpp:867
#, c-format
msgid "DTR %s: Delivery service at %s can copy to %s"
msgstr "DTR %s: Служба доставки в %s может копировать в %s"

#: src/libs/data-staging/Scheduler.cpp:876
#, c-format
msgid "DTR %s: Delivery service at %s can copy from %s"
msgstr "DTR %s: Служба доставки в %s может копировать из %s"

#: src/libs/data-staging/Scheduler.cpp:890
#, c-format
msgid ""
"DTR %s: Could not find any useable delivery service, forcing local transfer"
msgstr ""
"DTR %s: Не удалось обнаружить подходящую службу доставки, вынужденно "
"используется локальная пересылка"

#: src/libs/data-staging/Scheduler.cpp:913
#, c-format
msgid "DTR %s: Not using delivery service %s due to previous failure"
msgstr "DTR %s: Служба доставки %s не используется в связи с предыдущим сбоем"

#: src/libs/data-staging/Scheduler.cpp:924
#, c-format
msgid "DTR %s: No remote delivery services are useable, forcing local delivery"
msgstr ""
"DTR %s: Ни одна из удалённых служб доставки не подходит, вынужденно "
"используется локальная доставка"

